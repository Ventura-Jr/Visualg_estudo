Algoritmo "recebe nrs e realiza operações matemática"
//exercícios 1,2,3 e 4 do dia 07-03-23

Var

Num1, Num2, soma, sub, divi, mult : real

Inicio

      escreval("Digite primeiro número: ")
      leia(Num1)
      
      escreval("Digite o segundo número: ")
      leia(Num2)
      
      soma <- Num1 + Num2
      sub <- Num1 - Num2
      divi <- Num1 / Num2
      mult <- Num1 * Num2
      
      escreval("A soma dos números é igual a: " ,soma)
      escreval("A subtração dos números é igual a: " ,sub)
      escreval("A divisão dos números é igual a: " ,divi)
      escreval("A multiplicação dos números é igual a: " ,mult)

